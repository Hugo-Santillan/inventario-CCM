{% extends 'InventarioBundle::layout.html.twig' %}
{% block titulo %}
   Resultado encontrado {{ entity.categoria }}
{% endblock %}
{% block body -%}

    <table class="table table-striped table-bordered table-hover">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Numero de Inventario</th>
            <td>{{ entity.noInventario }}</td>
        </tr>
        <tr>
            <th>Folio Sicop</th>
            <td>{{ entity.folioSicop }}</td>
        </tr>
        <tr>
            <th>Fecha de Adquisicion</th>
            <td>{{ entity.fechaAdq|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Descripción</th>
            <td>{{ entity.descripcion }}</td>
        </tr>
        <tr>
            <th>Marca</th>
            <td>{{ entity.marca }}</td>
        </tr>
        <tr>
            <th>Modelo</th>
            <td>{{ entity.modelo }}</td>
        </tr>
        <tr>
            <th>Serie</th>
            <td>{{ entity.serie }}</td>
        </tr>
        <tr>
            <th>Comentario</th>
            <td>{{ entity.comentario }}</td>
        </tr>
        <tr>
            <th>Ubicación</th>
            <td>{{ entity.ubicacion }}</td>
        </tr>
        <tr>
            <th>Valido Fisicamente</th>
            <td>{% if entity.validoFis %} Si {% else %} No {% endif %}</td>
        </tr>
        <tr>
            <th>Tipo de Adquicision</th>
            <td>{{ entity.tipoAdq }}</td>
        </tr>
        <tr>
            <th>Estatus</th>
            <td>{% if entity.estatus %} Activo {% else %} Inactivo {% endif %}</td>
        </tr>
        <tr>
            <th>Foto</th>
            <td>{{ entity.foto }}</td>
        </tr>
        <tr>
            <th>Creado</th>
            <td>{{ entity.created|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Modificado</th>
            <td>{{ entity.modified|date('Y-m-d H:i:s') }}</td>
        </tr>
        </tbody>
    </table>


{% endblock %}
