{% extends 'InventarioBundle::layout.html.twig' %}
{% block titulo %}
{% endblock %}
{% block body -%}

    {% for entity in entities %}
    <table class="table table-striped table-bordered table-hover">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Numero de Inventario</th>
            <td>{{ entity.inventario }}</td>
        </tr>
        <tr>
            <th>Folio Sicop</th>
            <td>{{ entity.sicop }}</td>
        </tr>
        <tr>
            <th>Fecha de Adquisicion</th>
            <td>{{ entity.adquisicion|date('Y-m-d') }}</td>
        </tr>
        <tr>
            <th>Descripción</th>
            <td>{{ entity.descripcion }}</td>
        </tr>
        <tr>
            <th>Marca</th>
            <td>{{ entity.marca }}</td>
        </tr>
        <tr>
            <th>Modelo</th>
            <td>{{ entity.modelo }}</td>
        </tr>
        <tr>
            <th>Serie</th>
            <td>{{ entity.serie }}</td>
        </tr>
        <tr>
            <th>Comentario</th>
            <td>{{ entity.comentario }}</td>
        </tr>
        <tr>
            <th>Ubicación</th>
            <td>{{ entity.ubicacion }}</td>
        </tr>
        <tr>
            <th>Valido Fisicamente</th>
            <td>{% if entity.valido %} Si {% else %} No {% endif %}</td>
        </tr>
        <tr>
            <th>Estatus</th>
            <td>{% if entity.estatus %} Activo {% else %} Inactivo {% endif %}</td>
        </tr>
        <tr>
            <th>Foto</th>
            <td>{{ entity.foto }}</td>
        </tr>
        <tr>
            <th>Creado</th>
            <td>{{ entity.creado|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Modificado</th>
            <td>{{ entity.modificado|date('Y-m-d H:i:s') }}</td>
        </tr>
        </tbody>
    </table>
{% endfor %}


{% endblock %}
